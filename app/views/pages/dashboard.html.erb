<!-- dashboard.html.erb -->

User Dashboard

<div>
  <% if @bookings %>
    <% @bookings.each do |booking|%>

      <%= booking %>
    <% end %>
  <% else %>
    <p>No bookings</p>
  <% end %>
</div>

<br>
<br>

<div>
  <% if @studios %>
    <% @studios.each do |studio|%>
      <%= studio.name %>
      <%= cl_image_tag studio.photo, height: 300, width: 400, crop: :fill %>
    <% end %>
  <% else %>
    <p>No studios</p>
  <% end %>
</div>
